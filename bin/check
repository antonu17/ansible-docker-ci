#!/bin/bash
set -e

SCRIPT_DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source ${SCRIPT_DIR}/helper

exec 3>&1
exec 1>&2

# Change dir to $1
cd "$1"
touch 1.txt
pwd
echo "EXIT"
exit 1
# Creat temp folder 
temp_dir=`mktemp -d`
payload=$(mktemp ${temp_dir}/resource-in.XXXXXX)
# Load stdin in $payload
cat > $payload <&0

check=$(parse_json '.source.check')
script=$(parse_json '.source.script')

echo "[ {"version": "false" }]"


